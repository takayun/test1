<div class="card">
  <%= link_to image_tag(prototype.image, class: :card__img ), root_path%>
  <div class="card__body">
    <%= link_to prototype.car_maker, root_path, class: :card__title%>
    <p class="card__summary">
      <%= prototype.car_model %>
    </p>
    <%= link_to "by " + prototype.user.shop_name, user_path(prototype.user.id), class: :card__user %>
  </div>
</div>
